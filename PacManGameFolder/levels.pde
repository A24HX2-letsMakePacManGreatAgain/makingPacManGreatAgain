char[][][] levels = { level1, level2, level3, level4, level5, level6};
int currentLevel = 0;
char[][] playingBoard2 = levels[currentLevel];

//denne boolean fortækker hvis der er fundet nogle c'er så skal den ikke skifte bane, men hvis der ikke er nogen skal den gå videre til næste level
void changeLevel() 
{
  boolean cFound = false;

  for (int i = 0; i < playingBoard2.length; i++) 
  {
    for (int j = 0; j < playingBoard2[i].length; j++)
    {
      if (playingBoard2[i][j] == 'c') 
      {
        cFound = true;
        break;
      }
    }
    if (cFound) break;
  }

  if (!cFound && currentLevel < levels.length - 1) 
  {
    currentLevel++;
    playingBoard2 = levels[currentLevel];
    println("Skiftede til level: " + currentLevel);
    resetGhostPosition();
    resetPacmanPosition();
  }
}

//dette fortæller programmet at hvergang den går videre til næste level så placere den spøgelset ved start igen (eller der hvor g står på brættet)
void resetGhostPosition() 
{
  for (int i = 0; i < playingBoard2.length; i++) 
  {
    for (int j = 0; j < playingBoard2[i].length; j++) 
    {
      if (playingBoard2[i][j] == 'g') 
      {
        ghost.PBposX = j;
        ghost.PBposY = i;
        playingBoard2[i][j] = ' '; // fjerner 'g' hvis det kun er en startmarkør
        return;
      }
    }
  }
}

// dette er det samme som spøgelset, men her er det bare med pacman og p.
void resetPacmanPosition()
{
  for (int i = 0; i < playingBoard2.length; i++)
  {
    for (int j = 0; j < playingBoard2[i].length; j++) 
    {
      if (playingBoard2[i][j] == 'p') 
      {
        pacman.PBposX = j;
        pacman.PBposY = i;
        playingBoard2[i][j] = ' '; // fjerner 'p' fra brættet hvis det kun var en markør
        return;
      }
    }
  }
}
